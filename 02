//Write a function to find the smallest number in given array
#include<stdio.h>
int SmallestNumber (int a[])
{
    int smallest=a[0];
    for(int i=1;i<10;i++)
    {
        if(smallest>a[i])
            smallest=a[i];
    }
    return smallest;
}
int main()
{
    int a[10],i;
    printf("Enter any 10 numbers : ");
    for(i=0;i<10;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("\n\n\t %d is the smallest number.\n\n",SmallestNumber(a));
    return 0;
}
